{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "env-cmd -f .env.dev react-scripts start",
    "stg": "env-cmd -f .env.stg react-scripts start",
    "start": "env-cmd -f .env.prd react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "serve": "npx serve -s build",
    "build:stg": "env-cmd -f .env.stg react-scripts build && yarn sentry:sourcemaps-stg",
    "build:prd": "env-cmd -f .env.prd react-scripts build && yarn sentry:sourcemaps-prd",
    "sentry:sourcemaps-stg": "sentry-cli sourcemaps inject --org uncharted-49 --project gangster-arena-stg ../firebase/build/stg/app && sentry-cli sourcemaps upload --org uncharted-49 --project gangster-arena-stg ../firebase/build/stg/app",
    "sentry:sourcemaps-prd": "sentry-cli sourcemaps inject --org uncharted-49 --project gangster-arena-prd ../firebase/build/prd/app && sentry-cli sourcemaps upload --org uncharted-49 --project gangster-arena-prd ../firebase/build/prd/app"
  },
  "dependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@ethersproject/address": "^5.7.0",
    "@ethersproject/contracts": "^5.7.0",
    "@faker-js/faker": "^8.3.1",
    "@metamask/detect-provider": "^2.0.0",
    "@mui/icons-material": "^5.14.13",
    "@mui/material": "^5.14.13",
    "@privy-io/react-auth": "^1.43.4",
    "@sentry/cli": "^2.23.0",
    "@sentry/react": "^7.86.0",
    "@tanstack/react-query": "^4.36.1",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^13.5.0",
    "@uniswap/v2-core": "^1.0.1",
    "@uniswap/v2-periphery": "^1.1.0-beta.0",
    "axios": "^1.5.1",
    "env-cmd": "^10.1.0",
    "firebase": "^10.5.0",
    "moment": "^2.29.4",
    "notistack": "^3.0.1",
    "phaser": "^3.70.0",
    "phaser3-rex-plugins": "^1.60.7",
    "react": "^18.2.0",
    "react-countdown": "^2.3.5",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.17.0",
    "react-scripts": "5.0.1",
    "source-map-loader": "^4.0.1",
    "superstruct": "^1.0.3",
    "web-vitals": "^2.1.4",
    "workbox-background-sync": "^6.4.2",
    "workbox-broadcast-update": "^6.4.2",
    "workbox-cacheable-response": "^6.4.2",
    "workbox-core": "^6.4.2",
    "workbox-expiration": "^6.4.2",
    "workbox-google-analytics": "^6.4.2",
    "workbox-navigation-preload": "^6.4.2",
    "workbox-precaching": "^6.4.2",
    "workbox-range-requests": "^6.4.2",
    "workbox-routing": "^6.4.2",
    "workbox-strategies": "^6.4.2",
    "workbox-streams": "^6.4.2",
    "zustand": "^4.4.3"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
